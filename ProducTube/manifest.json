{
    "name": "ProducTube",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["*://*.youtube.com/*"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "vidCount"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "*://*.youtube.com/*",
      "tabs",
      "storage",
      "alarms",
      "webNavigation"
    ]

